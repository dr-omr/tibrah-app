{
    "name": "LabResult",
        "type": "object",
            "properties": {
        "test_name": {
            "type": "string",
                "description": "اسم الفحص"
        },
        "test_name_en": {
            "type": "string",
                "description": "اسم الفحص بالإنجليزية"
        },
        "category": {
            "type": "string",
                "enum": [
                    "blood",
                    "hormones",
                    "vitamins",
                    "minerals",
                    "liver",
                    "kidney",
                    "thyroid",
                    "sugar",
                    "lipids",
                    "other"
                ],
